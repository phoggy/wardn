#!/usr/bin/env bash

# rayvn package

projectName='wardn'
projectVersion='0.1.0'
projectReleaseDate=''

declare -gA projectDependencies=(

    [rayvn_min]='0.1.2'
    [rayvn_extract]=1
    [rayvn_brew]=true
    [rayvn_brew_tap]='phoggy/rayvn'
    [rayvn_url]='https://github.com/phoggy/rayvn'

    [valt_min]='0.1.0'
    [valt_extract]=1
    [valt_brew]=true
    [valt_brew_tap]='phoggy/valt'
    [valt_url]='https://github.com/phoggy/valt'

    [bw_min]='2025.2.0'
